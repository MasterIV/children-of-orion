<table class="table">
	<thead>
	<tr>
		<th>Installed mod pints:</th>
		{% for i, q in data %}
			{% if i < 11 %}
				<th>{{ i }}</th>
			{% endif %}
		{% endfor %}
	</tr>
	</thead>

	<tbody>
		<tr>
			<td>Cost multiplier:</td>
			{% for i, q in data %}
				{% if i < 11 %}
					<td>{{ q }}</td>
				{% endif %}
			{% endfor %}
		</tr>
	</tbody>
</table>